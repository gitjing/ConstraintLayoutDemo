<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ljj.constraintlayoutdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".demolist.RelativePositioningActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".demolist.MarginsActivity" />
        <activity
            android:name=".demolist.CenteringAndBiasActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".demolist.CircularPositioningActivity" />
        <activity android:name=".demolist.MatchConstraintActivity" />
        <activity android:name=".demolist.RatioActivity" />
        <activity android:name=".demolist.ChainsActivity" />
        <activity android:name=".demolist.GuideLineActivity"></activity>
    </application>

</manifest>